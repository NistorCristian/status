<!-- The main content area -->
<main id="main" role="main">

	<!-- DEVELOPMENT-OPEN-SOURCE -->
	<section class="development-open-source">
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					<p class="statement">An Open Source Community</p>
					<h1>Development</h1>
					<p class="teaser-1">Welcome to the Status Developer Portal. Find everything you need to start contributing to Status, building your own DApps, or extending your work with the natives features of Status.</p>
				</div>
			</div>
		</div>
	</section>
	<!-- END-DEVELOPMENT-OPEN-SOURCE -->

	<!-- DEVELOPMENT-QUICK-NAV -->
	<section class="quick-nav">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ul>
						<li><a href="#" class="active"><%- image_tag("/img/development-small-icon.svg") %>Development</a></li>
						<li><a href="#"><%- image_tag("/img/educate-small-icon.svg") %>Educate</a></li>
						<li><a href="#"><%- image_tag("/img/community-groups-small-icon.svg") %>Community Groups</a></li>
					</ul>
				</div>
			</div>
		</div>
	</section>
	<!-- END-DEVELOPMENT-QUICK-NAV -->

	<!-- DEVELOPMENT-QUICK-NAV-SUB -->
	<section class="quick-nav quick-nav-sub">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ul>
						<li><a href="#">Contribute to Status</a></li>
						<li><a href="#">Build  DApps</a></li>
						<li><a href="#">Extend  DApps</a></li>
						<li><a href="#">Developer Resources</a></li>
					</ul>
					<a href="#"><%- image_tag("/img/github-white-logo.svg") %>Github</a>
				</div>
			</div>
		</div>
	</section>
	<!-- END-DEVELOPMENT-QUICK-NAV-SUB -->

	<!-- DEVELOPMENT-CONTRIBUTE-TO-STATUS -->
	<section class="development-contribute-to-status p-t-160 p-b-160">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h2>Contribute to Status</h2>
					<p class="teaser-2">Install Status and get set up with this <a href="#" class="link-arrow quick-start-guide">Quick Start Guide</a></p>
				</div>
			</div>
		</div>
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					<div class="box-with-icon">
						<%- image_tag("/img/github-black-logo.svg") %>
						<div class="inner">
							<h3>Status-React</h3>
							<p>Written in Clojure - this is where we handle interactions between Whisper and the app; between the wallet, the user and any Distributed Application (DApp) that they want to access; and where the UI logic lives.</p>
							<a href="#" class="link-arrow">View Repository</a>
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="box-with-icon">
						<%- image_tag("/img/github-black-logo.svg") %>
						<div class="inner">
							<h3>Status-Go</h3>
							<p>Written in Go - this is where all of the blockchain logic, transactions, and light client work we do lives. </p>
							<a href="#" class="link-arrow">View Repository</a>
						</div>
					</div>
				</div>
			</div>
		</div>

		<div class="container">
			<div class="row">
				<div class="col-md-6">
					<div class="box-with-icon m-t-50">
						<%- image_tag("/img/crypto-logo.svg") %>
						<div class="inner">
							<h3>Earn crypto for <br>completing bounties</h3>
							<p>See the full list of technical bounties on <a href="#" class="link-arrow">Gitcoin</a></p>
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="box-with-icon m-t-50">
						<%- image_tag("/img/bounty-program.svg") %>
						<div class="inner">
							<h3>Report a security <br>vulnerability and fix it</h3>
							<p>Learn more about the <a href="#" class="link-arrow">Bug bounty program</a></p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- END-DEVELOPMENT-CONTRIBUTE-TO-STATUS -->

	<!-- DEVELOPMENT-DAPP -->
	<section class="development-dapp">
		<div class="container">
			<div class="row">
				<div class="col-md-6 development-build-dapp">
					<div class="inner d-flex align-items-start flex-column h-100">
						<div class="row">
							<div class="col-md-11 col-lg-10">
								<h2>Build your own DApp</h2>
								<p class="teaser-2">Get started with our tools and frameworks to build your own DApp.</p>
								<ul>
									<li><a href="#" class="link-arrow">Learn more about the Javascript API</a></li>
									<li><a href="#" class="link-arrow">Build your own DApp with Embark Framework</a></li>
									<li><a href="#" class="link-arrow">Tutorials</a></li>
									<li><a href="#" class="link-arrow">Submit your DApp</a></li>
								</ul>
							</div>
						</div>
					</div>
				</div>
				<div class="col-md-6 col-black col-offset-top development-extend-dapp home-private">
					<div class="inner d-flex align-items-start flex-column justify-content-center h-100">
						<div class="row">
							<div class="col-lg-11 d-flex align-items-start flex-column">
								<h2>Extend your DApp</h2>
								<p class="teaser-2">Extensions allow you to leverage the native of features of Status within your DApp</p>
								<ul>
									<li><a href="#" class="link-arrow link-arrow-white">Quick Start Guide</a></li>
									<li><a href="#" class="link-arrow link-arrow-white">Tutorials</a></li>
									<li><a href="#" class="link-arrow link-arrow-white">Examples</a></li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- DEVELOPMENT-DAPP -->

	<!-- DEVELOPMENT-RESOURCES -->
	<section class="development-resources col-dark-grey col-black">
		<div class="container">
			<div class="row">
				<div class="col-md-6 contribute-support">
					<div class="inner">
						<h2>Contribute Support</h2>
						<ul>
							<li>
								<a href="#" class="link-arrow link-arrow-white">Write Documentation</a>
								<p>Dive into the docs and help write docs and fix errors and typos.</p>
							</li>
							<li>
								<a href="#" class="link-arrow link-arrow-white">Translate Documentation</a>
								<p>We need help translating all of our documentation into your local language. We use Lokalise for translations.</p>
							</li>
							<li>
								<a href="#" class="link-arrow link-arrow-white">Welcome new contributors</a>
								<p>Jump into the Status #introductions channel and help out new contributors.</p>
							</li>
							<li>
								<a href="#" class="link-arrow link-arrow-white">Create Tutorials</a>
								<p>Create a new blog post or record a video of a new tutorial.</p>
							</li>
						</ul>
					</div>
				</div>
				<div class="col-md-6 developer-resources">
					<div class="inner">
						<h2>Developer Resources</h2>
						<ul>
							<li><a href="#" class="link-arrow link-arrow-white">Github Repositories</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Documentation</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Embark</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Tutorials</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Nightly Builds</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Blog</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Join the bi-weekly Developer Call</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">Latest Release notes - v0.9.35</a></li>
							<li><a href="#" class="link-arrow link-arrow-white">List of Active Swarms</a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- DEVELOPMENT-RESOURCES -->

	<!-- DEVELOPMENT-GET-IN-TOUCH -->
	<section class="development-get-in-touch p-t-160 p-b-160">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h3>Get in touch</h3>
				</div>
			</div>
			<div class="row">
				<div class="col-md-3">
					<div class="inner">
						<a href="#"><%- image_tag("/img/status-purple-logo.svg") %>Join #Status</a>
						<p>General Status news and discussion.</p>
					</div>
				</div>
				<div class="col-md-3">
					<div class="inner">
						<a href="#"><%- image_tag("/img/status-purple-logo.svg") %>Join #Status-educate</a>
						<p>Discussion about education around Status.</p>
					</div>
				</div>
				<div class="col-md-3">
					<div class="inner">
						<a href="#"><%- image_tag("/img/status-purple-logo.svg") %>Join #introductions</a>
						<p>New to Status? Introduce yourself and say hi. Someone will be there to help you out.</p>
					</div>
				</div>
				<div class="col-md-3">
					<div class="inner">
						<a href="#">Contact us</a>
						<p>For everything else, contact us directly.</p>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- END-DEVELOPMENT-GET-IN-TOUCH -->


</main>