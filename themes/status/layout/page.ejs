<!-- The main content area -->
<main id="main" role="main">

	<section class="page-intro p-t-160">
		<div class="container">
			<div class="row">
				<div class="col-md-12 col-xl-8 ml-auto mr-auto">
					<h1><%= page.title %></h1>
					<p class="teaser-1"><%= page.description %></p>
				</div>
			</div>
		</div>
	</section>

	<section class="page-content p-b-160">
		<div class="container">
			<div class="row">
				<div class="col-md-12 col-xl-8 ml-auto mr-auto">
					<%- page.content %>
				</div>
			</div>
		</div>
	</section>

</main>